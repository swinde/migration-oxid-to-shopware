<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
           http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="MigrationSwinde\MigrationOxidToShopware\Service\ProductMigrator">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
        </service>

        <service id="MigrationSwinde\MigrationOxidToShopware\Command\MigrationCommand" public="true">
            <argument type="service" id="MigrationSwinde\MigrationOxidToShopware\Service\ProductMigrator"/>
            <tag name="console.command"/>
        </service>
    </services>
</container>
